    "@types/react-dom": "18.0.8",
    "autoprefixer": "^10.4.13",
    "browserslist": "^4.21.4",
    "concurrently": "^7.6.0",
    "eslint": "8.26.0",
    "eslint-config-next": "^13.1.0",
    "postcss": "^8.4.19",
    "sass": "^1.56.1",
    "scss": "^0.2.4",
